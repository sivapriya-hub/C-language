#include <stdio.h>

int main()
{
int a,b,c,second_largest;
printf("enter the values");
scanf("%d%d%d",&a,&b,&c);
if((a>b && a<c) || (a>c && a<b)){
    second_largest=a;
}
else if((b>a && b<c) || (b>c && b<a)){
    second_largest=b;
}
else{
    second_largest=c;
}
printf("the second largest number is %d",second_largest);
}
