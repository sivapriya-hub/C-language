#include <stdio.h>
#include<limits.h>
#include<stdbool.h>
void main() {
    int n;
    int b[n];
    printf("enter the size of an array");
    scanf("%d",&n);
    int a[n];
    printf("enter the array elements",n);
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++){
        b[i] = a[i];
    }
    for(int i=0;i<n;i++){
        printf("%d",b[i]);
    }
}
