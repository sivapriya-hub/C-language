#include <stdio.h>
#include<limits.h>
#include<stdbool.h>
void main() {
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int j = n-1;
    for(int i=0;i<n/2;i++){
        int temp=a[i];
        a[i]=a[j];
        a[j]=temp;
        j--;
    }
    for(int i=0;i<n;i++){
        printf("%d",a[i]);
    }
    
}
